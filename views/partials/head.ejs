<script>
$(document).on('lity:ready', function(event, lightbox) {

    $(document).find('.lity-wrap').after('<div class="buttons zoom-controls">  <button class="zoom-in btn btn-info zoom-button-right">Zoom In</button> <button class="zoom-out btn btn-info zoom-button-left">Zoom Out</button> <input type="range" class="zoom-range"> <button class="reset">Reset</button> </div> ');
    
    var $section = $(this).find('.lity-content img');
    $(this).find('.lity-content img').panzoom();
var currentZoom = 1;
    $('.zoom-in').click(function(){
        $('.lity-content img').animate({ 'zoom': currentZoom += .5 }, 400);
        $('.lity-content img').css("-moz-transform", "scale(1.75)");  
    });
    $('.zoom-out').click(function(){
        $('.lity-content img').animate({ 'zoom': currentZoom -= .5 }, 400);
        $('.lity-content img').css("-moz-transform", "scale(1)");  
    });
});
    
</script>


  